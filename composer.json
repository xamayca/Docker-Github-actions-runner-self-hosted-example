{
  "name": "xamayca/docker-github-actions-runner-self-hosted-example",
  "description": "Démo d’un runner GitHub Actions auto-hébergé dans un conteneur Docker.",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "xamayca",
      "email": "christopher.hariouat@gmail.com",
      "role": "Developer",
      "homepage": "https://github.com/xamayca"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "require": {
    "php": "^8.4"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^12.1"
  }
}
